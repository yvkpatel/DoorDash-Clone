{
    "name": "map-module",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@react-google-maps/api": "^2.5.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "identity-obj-proxy": "^3.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3",
        "web-vitals": "^1.1.2"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/components/Sidepanel.js",
            "\\.(css|less)$": "identity-obj-proxy"
        },
        "setupFilesAfterEnv": [
            "./src/setup-tests.js"
        ]
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "jest",
        "update": "jest -u",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
        "enzyme": "^3.11.0",
        "jest": "^27.3.1",
        "jest-environment-jsdom": "^27.0.6",
        "react-test-renderer": "^17.0.2"
    }
}
